{
  "logging": {
    "level": "DEBUG",
    "file": "training.log",
    "format": "%(asctime)s - %(levelname)s - %(message)s"
  },
  "monitoring": {
    "track_gradients": true,
    "gradient_threshold": 1e-5,
    "log_interval": 10
  },
  "parameters": {
    "max_epochs": 800,
    "early_stopping": {
      "patience": 30,
      "delta": 0.0001
    }
  }
}